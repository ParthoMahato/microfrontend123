{
  "name": "fresh-cart-application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /microfrontend && ng build address && ng build landing && ng build login && ng build mycart && ng build orders",
    "build:prod:all": "ng build --base-href /microfrontend && ng build --configuration production --project=address && ng build --configuration production --project=landing && ng build --configuration production --project=login && ng build --configuration production --project=mycart && ng build --configuration production --project=orders",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.2.0",
    "@angular-architects/module-federation-tools": "^14.2.0",
    "@angular/animations": "12.2.0",
    "@angular/common": "12.2.0",
    "@angular/compiler": "12.2.0",
    "@angular/core": "12.2.0",
    "@angular/forms": "12.2.0",
    "@angular/platform-browser": "12.2.0",
    "@angular/platform-browser-dynamic": "12.2.0",
    "@angular/elements": "12.2.0",
    "@angular/router": "12.2.0",
    "@ngrx/store": "12.2.0",
    "@ngrx/effects": "12.2.0",
    "bootstrap": "5.1.3",
    "concurrently": "^5.3.0",
    "flexboxgrid": "^6.3.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.0",
    "@angular/cli": "12.0.0",
    "@angular/compiler-cli": "12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.0.0-rc.0",
    "ngx-build-plus": "11.0.0",
    "protractor": "~7.0.0",
    "serve": "^11.3.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.3"
  }
}